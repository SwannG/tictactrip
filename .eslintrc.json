{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "experimentalObjectRestSpread": true,
        "sourceType": "module"
    },
    "settings": {
        "import/resolver": {
            "babel-module": [
                "module-resolver",
                {
                    "root": [
                        "./src"
                    ]
                }
            ]
        }
    },
    "plugins": [
        "babel",
        "eslint-plugin-standard"
    ],
    "rules": {
        "arrow-body-style": 0,
        "array-bracket-spacing": 0,
        "function-paren-newline": 0,
        "array-callback-return": 0,
        "arrow-parens": [
            0,
            "as-needed"
        ],
        "block-spacing": 0,
        "brace-style": 0,
        "camelcase": 0,
        "no-process-env": 0,
        "capitalized-comments": 0,
        "class-methods-use-this": 0,
        "curly": 0,
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": 0,
        "func-style": 1,
        "id-length": 0,
        "indent": [
            2,
            4
        ],
        "init-declarations": 0,
        "linebreak-style": [
            0,
            "unix"
        ],
        "max-len": [
            2,
            200
        ],
        "max-params": 0,
        "max-statements": 0,
        "max-statements-per-line": 0,
        "multiline-ternary": 0,
        "new-cap": [
            2,
            {
                "properties": false
            }
        ],
        "newline-after-var": 0,
        "newline-before-return": 0,
        "newline-per-chained-call": 0,
        "no-confusing-arrow": 0,
        "no-console": 0,
        "no-continue": 0,
        "no-duplicate-imports": 0,
        "no-empty-function": 0,
        "no-extra-parens": 0,
        "no-extra-semi": 0,
        "no-implicit-coercion": 0,
        "no-invalid-this": 0,
        "no-lonely-if": 0,
        "no-magic-numbers": 0,
        "no-mixed-operators": 0,
        "no-multi-assign": 0,
        "no-negated-condition": 0,
        "no-plusplus": 0,
        "no-prototype-builtins": 0,
        "no-ternary": 0,
        "no-undefined": 0,
        "no-unused-vars": 0,
        "no-undef": 0,
        "no-useless-constructor": 0,
        "no-useless-escape": 0,
        "no-useless-rename": 0,
        "no-useless-return": 0,
        "no-warning-comments": 1,
        "object-curly-newline": 0,
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "object-property-newline": 0,
        "object-shorthand": 0,
        "one-var": 0,
        "operator-linebreak": 0,
        "padded-blocks": 0,
        "prefer-const": 1,
        "prefer-destructuring": 0,
        "prefer-template": 0,
        "quote-props": [
            2,
            "as-needed"
        ],
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ],
        "require-jsdoc": 0,
        "sort-keys": 0,
        "sort-imports": 0,
        "sort-vars": 0,
        "lines-around-comment": [
            "error",
            {
                "beforeBlockComment": true
            }
        ],
        "space-before-function-paren": [
            "error",
            "never"
        ],
        "spaced-comment": 0,
        "space-in-parens": [
            "error",
            "always"
        ],
        "semi": [
            "error",
            "always"
        ],
        "valid-jsdoc": 0,
        "computed-property-spacing": [
            "error",
            "always"
        ]
    }
}